<% if (alerts.error) { %> 
    <% alerts.error.forEach(msg => { %>
        <div class="alert alert-warning" role="alert">
            <%= msg  %> 
    <% }) %> 
<% } %>
<% if (alerts.success) { %> 
    <% alerts.success.forEach(msg => { %>
     
        <div class="alert alert-success" role="alert">
            <%= msg  %> 
       
    <% }) %> 
<% } %> 